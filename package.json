{
	"name": "create-a8-app",
	"version": "1.1.1",
	"author": "Alexandre Sanchez (@alex73630)",
	"license": "MIT",
	"description": "Quickest way to start a new TypeScript app, inspired by create-t3-app",
	"repository": {
		"type": "git",
		"url": "https://github.com/alex73630/create-a8-app.git"
	},
	"type": "module",
	"exports": "./dist/index.js",
	"bin": {
		"create-a8-app": "./dist/index.js"
	},
	"engines": {
		"node": ">=18"
	},
	"packageManager": "npm@9.6.2",
	"scripts": {
		"typecheck": "tsc",
		"build": "tsup",
		"dev": "tsup --watch",
		"clean": "rm -rf dist .turbo node_modules",
		"start": "node dist/index.js",
		"lint": "eslint . --report-unused-disable-directives",
		"lint:fix": "npm run lint -- --fix",
		"format": "prettier --write --plugin-search-dir=. **/*.{cjs,mjs,ts,tsx,md,json} --ignore-unknown --no-error-on-unmatched-pattern",
		"format:check": "npm run format -- --check",
		"semantic-release": "semantic-release",
		"prepare": "is-ci || husky install",
		"commit": "cz"
	},
	"keywords": [
		"create-a8-app",
		"alex73630",
		"a8-stack",
		"typescript"
	],
	"devDependencies": {
		"@commitlint/cli": "^17.6.1",
		"@commitlint/config-conventional": "^17.6.1",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@types/eslint": "^8.37.0",
		"@types/inquirer": "^9.0.3",
		"@types/lint-staged": "^13.2.0",
		"@types/node": "^18.16.3",
		"@types/prettier": "^2.7.2",
		"@typescript-eslint/eslint-plugin": "^5.59.1",
		"@typescript-eslint/parser": "^5.59.1",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^8.39.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-import": "^2.27.5",
		"eslint-plugin-prettier": "^4.2.1",
		"husky": "^8.0.3",
		"is-ci": "^3.0.1",
		"lint-staged": "^13.2.2",
		"prettier": "^2.8.8",
		"semantic-release": "^21.0.2",
		"tsup": "^6.7.0"
	},
	"dependencies": {
		"@types/fs-extra": "^11.0.1",
		"@types/gradient-string": "^1.1.2",
		"chalk": "^5.2.0",
		"commander": "^10.0.1",
		"execa": "^7.1.1",
		"fs-extra": "^11.1.1",
		"gradient-string": "^2.0.2",
		"handlebars": "^4.7.7",
		"inquirer": "^9.2.0",
		"ora": "^6.3.0",
		"sort-package-json": "^2.4.1"
	},
	"config": {
		"commitizen": {
			"path": "./node_modules/cz-conventional-changelog"
		}
	}
}
